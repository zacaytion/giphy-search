{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "rxjs-tslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double", "avoid-escape"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "jsx-boolean-value": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}